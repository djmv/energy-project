  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render template: "layouts/_navbar.html.erb"%>
	<div id="main-container" class="container-fluid">
    <div class="row">
      <div class="col-xs-3 col-md-3">
        <h3 class="centered">Ámbar</h3>
        <div class="list-group">
          <a href="#" class="list-group-item active">Tiempo real</a>
          <a href="#" class="list-group-item list-group-item-action">Históricos</a>
        </div>
        <%= image_tag("103.png", class: "col-xs-12") %>  
      </div>
      <div class="col-xs-9 col-md-3">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
